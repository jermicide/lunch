{
    "version": "2.0",
    "extensions": {
      "http": {
        "routePrefix": "api"
      }
    },
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "maxTelemetryItemsPerSecond": 20
        }
      },
      "logLevel": {
        "default": "Information",
        "Host.Results": "Error",
        "Function": "Information",
        "Host.Aggregator": "Trace"
      }
    },
    "extensionBundle": {
      "id": "Microsoft.Azure.Functions.ExtensionBundle",
      "version": "[3.*, 4.0.0)"
    }
  }